      ],
      "source": [
        "plt.pie(       [ 0.8,   1.0,       1.2           ],\n",
        "        labels=[ \"red\", \"yellow\",  \"blue\"        ],\n",
        "        colors=[ \"red\", \"#ffff00\", (0.0,0.0,1.0) ]);"
      ]
    },
